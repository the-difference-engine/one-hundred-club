<div class="container">
  <div class="row">
  <h1 class="members-headers"><%= @member.title %> <%= @member.first_name %> <%= @member.middle_name %>
                              <%= @member.last_name %> <%= @member.suffix %></h1>
  </div>
  <br>
  <div class="row">
    <div class="col-xs-4 col-xs-offset-2">
      <%= link_to "Edit Member", "/members/#{@member.id}/edit", method: :get, class: 'btn btn-primary btn-block' %>
    </div>
    <div class="col-xs-4">
      <%= link_to "Delete Member", "/members/#{@member.id}", method: :delete, class: 'btn btn-danger btn-block' %>
    </div>
  </div>
</div>
